      <image>
        <Image url="@../images/Button-Close-icon.png" preserveRatio="false" smooth="false" />
      </image>
    </ImageView>
  </children>
  <effect>
    <DropShadow />
  </effect>
  <stylesheets>
    <URL value="@../css/Login.css" />
  </stylesheets>
